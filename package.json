{
  "name": "grafana-ydb-datasource",
  "version": "0.4.3",
  "description": "YDB datasource plugin for Grafana",
  "scripts": {
    "build": "rm -rf dist/ && npm run build:frontend && npm run build:backend && npm run sign",
    "build:frontend": "webpack -c ./webpack.config.ts --env production",
    "build:backend": "PATH=$(go env GOPATH)/bin:$PATH mage",
    "sign": "npx --yes @grafana/sign-plugin@latest",
    "dev": "webpack -w -c ./webpack.config.ts --env development",
    "test": "jest --watch --onlyChanged",
    "test:ci": "jest --passWithNoTests --maxWorkers 4",
    "typecheck": "tsc --noEmit",
    "lint": "eslint --cache --ignore-path ./.gitignore --ext .js,.jsx,.ts,.tsx .",
    "lint:fix": "npm run lint -- --fix",
    "e2e": "npx cypress install && npx grafana-e2e run",
    "e2e:update": "npx cypress install && npx grafana-e2e run --update-screenshots",
    "server": "docker-compose up --build",
    "prepare": "husky install"
  },
  "author": "YDB",
  "license": "Apache-2.0",
  "devDependencies": {
    "@babel/core": "^7.16.7",
    "@commitlint/config-conventional": "^17.6.5",
    "@grafana/e2e": "^9.5.3",
    "@grafana/e2e-selectors": "9.2.5",
    "@grafana/eslint-config": "^5.1.0",
    "@grafana/tsconfig": "^1.2.0-rc1",
    "@swc/core": "^1.3.75",
    "@swc/helpers": "^0.5.0",
    "@swc/jest": "^0.2.23",
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^12.1.4",
    "@types/glob": "^8.0.0",
    "@types/jest": "^29.2.2",
    "@types/lodash": "^4.14.188",
    "@types/node": "^18.11.9",
    "@typescript-eslint/eslint-plugin": "^5.42.1",
    "@typescript-eslint/parser": "^5.42.1",
    "commitlint": "^17.6.5",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.7.1",
    "eslint": "8.26.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jsdoc": "^39.6.2",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.26.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-webpack-plugin": "^3.1.1",
    "fork-ts-checker-webpack-plugin": "^7.2.0",
    "glob": "^8.0.3",
    "husky": "^8.0.3",
    "identity-obj-proxy": "3.0.0",
    "jest": "^29.3.1",
    "jest-environment-jsdom": "^29.3.1",
    "lint-staged": "^13.2.2",
    "nanoid": "^4.0.2",
    "prettier": "^2.5.0",
    "replace-in-file-webpack-plugin": "^1.0.6",
    "sass": "1.56.1",
    "sass-loader": "13.2.0",
    "sql-autocomplete-parsers": "^1.22.0",
    "style-loader": "3.3.1",
    "swc-loader": "^0.2.3",
    "ts-node": "^10.5.0",
    "tsconfig-paths": "^4.1.0",
    "typescript": "4.9.5",
    "webpack": "^5.69.1",
    "webpack-cli": "^4.9.2",
    "webpack-livereload-plugin": "^3.0.2"
  },
  "engines": {
    "node": ">=16"
  },
  "dependencies": {
    "@emotion/css": "11.10.5",
    "@grafana/data": "9.4.3",
    "@grafana/runtime": "9.4.3",
    "@grafana/ui": "^9.4.7",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "tslib": "^2.5.3"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix --quiet"
    ]
  }
}
